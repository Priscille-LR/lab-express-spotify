<div class='albums'>
  {{#each albums}}
    <div class='album-card'>
      {{#if this.images}}
      <img src={{this.images.1.url}} alt='' />
      {{else}}
        <img src='images/unknown-music.png' alt='' />
      {{/if}}
      <div class='card-content'>
        <h2>{{this.name}}</h2>
        <a href='{{this.id}}/tracks'>View Tracks</a>
      </div>
    </div>
  {{/each}}
</div>